                border-bottom: 2px solid #f39c12!important;
                border-left: 2px solid #f39c12!important;
            }

            .card_madre_comercio{
                border-bottom: 2px solid #17a2b8!important;
                border-left: 2px solid #17a2b8!important;
            }

            .card_madre_moto{
                border-bottom: 2px solid #dc3545!important;
                border-left: 2px solid #dc3545!important;
            }

            .card_madre_cementerio{
                border-bottom: 2px solid #6c757d!important;
                border-left: 2px solid #6c757d!important;
            }

            .card_madre_escribano{
                border-bottom: 2px solid #00a65a!important;
                border-left: 2px solid #00a65a!important;
            }

            .card_madre_juicio{
                 border-bottom: 2px solid #007bff!important;
                 border-left: 2px solid #007bff!important;
             }

            .card_madre_varios{
                border-bottom: 2px solid #e1e6ea!important;
                border-left: 2px solid #e1e6ea!important;
            }

            .card_inmueble{
                color: #f0ad4e!important;
            }

            .card_comercio{
                color: #17a2b8!important;
            }

            .card_moto{
                color: #dc3545!important;
            }

            .card_cementerio{
                color: #6c757d!important;
            }

            .card_escribano{
                color: #00a65a !important;
            }

            .card_juicio{
                color: #007bff !important;
            }

            .card_bd{
                padding: 0!important;
                background-color: #fff!important;
                border-top: none!important;
                border-style: hidden!important;
            }

