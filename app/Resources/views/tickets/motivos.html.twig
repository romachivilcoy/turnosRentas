                margin-bottom: 0.1em!important;
            }

            .cl_sombra{
                font-family: "Play";
                text-transform: uppercase!important;
                text-shadow: 1px 1px #000;
                font-size: 31px;
                font-weight: 600;
            }

            .sombra_numeros{
               /* font-family: "Righteous";*/
                color:#fff;
                text-shadow: 1px 1px #000;
                margin-right: 33px;
            }

