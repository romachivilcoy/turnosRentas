    {% extends 'basePublico.html.twig' %}

    {% block body %}

            <header style="margin-top: -10px;background-color: #33BB66; " >
                <nav class="navbar navbar-static-top" role="navigation" style="position:relative;">
                    <div class="col-2">
                         <span class="head-img" style="position: absolute;left: 0px; top: 8px;">
                            <img src="{{ asset('img/muni.png') }}" class="pt-3">
                        </span>
                        <h1 class="pt-2 display-3" style="font-size: 2.8rem; color: #f5f5f5; text-align: center;">
                            TURNOS RENTAS
                            <small style="color: #fff;">Seleccione una opción</small>
                        </h1>
                    </div>
                </nav>
            </header>

            <div class="container" style="height: 80vh;">
                <div class="loader" id="loader">  </div>

                <div class="row pt-1 justify-content-center align-items-center" style="margin-bottom: 20px;max-height: 60px;">
                    <div class="col-md-12" id="div_inmuebles" name="background: #fff!important; max-height: 50px;">
                        <div class="card bg-danger mb-2 card_txt {% if tipoTramite == "P"%}card_madre_inmueble {% endif %}{% if tipoTramite == "C"%}card_madre_comercio {% endif %}{% if tipoTramite == "M"%}card_madre_moto {% endif %}{% if tipoTramite == "T"%}card_madre_cementerio {% endif %}{% if tipoTramite == "E"%}card_madre_escribano {% endif %}{% if tipoTramite == "J"%}card_madre_juicio {% endif %}{% if tipoTramite == "V"%}card_madre_varios {% endif %}">
                            <div class="card-body card_bd">
                                {% if tipoTramite == "P"%}
                                    <span class="info-box-icon pb_1 h-100 mx-auto infoicono">
                                        <i class="ima_posicion fa fa-home cl_sombra card_inmueble" aria-hidden="true"></i>
                                    </span>
                                    <span class="info-box-icon mx-auto infospan">
                                      <h3 class="card_inmueble" style="font-family: 'Play'; font-weight: 600; font-size: 2.15rem; text-shadow: 1px 1px #000;width: 200px;" id="descrip_tramite">INMUEBLES</h3>
                                    </span>
                                    {% set TramiteSeleccionado='INMUEBLES' %}
                                {% endif %}
                                {% if tipoTramite == "C"%}
                                    <span class="info-box-icon pb_1 h-100 mx-auto infoicono">
                                        <i class="ima_posicion fa fa-shopping-cart cl_sombra card_comercio" aria-hidden="true"></i>
                                    </span>
                                    <span class="info-box-icon mx-auto infospan">
                                      <h3 class="card_comercio" style="font-family: 'Play'; font-weight: 600;font-size: 2.15rem; text-shadow: 1px 1px #000;width: 200px;" id="descrip_tramite">COMERCIOS</h3>
                                    </span>
                                    {% set TramiteSeleccionado='COMERCIOS' %}
                                {% endif %}
                                {% if tipoTramite == "M"%}
                                    <span class="info-box-icon pb_1 h-100 mx-auto infoicono">
                                        <i class="ima_posicion fa fa-motorcycle cl_sombra card_moto" aria-hidden="true"></i>
                                    </span>
                                    <span class="info-box-icon mx-auto infospan">
                                      <h3 class="card_moto" style="font-family: 'Play'; font-weight: 600;font-size: 2.15rem; text-shadow: 1px 1px #000;width: 200px;" id="descrip_tramite">RODADOS</h3>
                                    </span>
                                    {% set TramiteSeleccionado='RODADOS' %}
                                {% endif %}
                                {% if tipoTramite == "T"%}
                                    <span class="info-box-icon pb_1 h-100 mx-auto infoicono">
                                        <i class="ima_posicion fa fa-arrows cl_sombra card_cementerio" aria-hidden="true"></i>
                                    </span>
                                    <span class="info-box-icon mx-auto infospan">
                                      <h3 class="card_cementerio" style="font-family: 'Play'; font-weight: 600;font-size: 2.15rem; text-shadow: 1px 1px #000;width: 200px;" id="descrip_tramite">CEMENTERIO</h3>
                                    </span>
                                    {% set TramiteSeleccionado='CEMENTERIO' %}
                                {% endif %}
                                {% if tipoTramite == "E"%}
                                    <span class="info-box-icon pb_1 h-100 mx-auto infoicono">
                                        <i class="ima_posicion fa fa-user cl_sombra card_escribano" aria-hidden="true"></i>
                                    </span>
                                    <span class="info-box-icon mx-auto infospan">
                                      <h3 class="card_escribano" style="font-family: 'Play';font-weight: 600;font-size: 2.15rem; text-shadow: 1px 1px #000;width: 200px;" id="descrip_tramite">ESCRIBANOS</h3>
                                    </span>
                                    {% set TramiteSeleccionado='ESCRIBANO' %}
                                {% endif %}

                                {% if tipoTramite == "J"%}
                                    <span class="info-box-icon pb_1 h-100 mx-auto infoicono">
                                        <i class="ima_posicion fa fa-balance-scale cl_sombra card_juicio" aria-hidden="true"></i>
                                    </span>
                                    <span class="info-box-icon mx-auto infospan">
                                      <h3 class="card_juicio" style="font-family: 'Play';font-weight: 600;font-size: 2.15rem; text-shadow: 1px 1px #000;width: 200px;" id="descrip_tramite">JUICIOS</h3>
                                    </span>
                                    {% set TramiteSeleccionado='JUICIO' %}
                                {% endif %}

                                {% if tipoTramite == "V"%}
                                    <span class="info-box-icon pb_1 h-100 mx-auto infoicono">
                                        <i class="ima_posicion fa fa-folder-open cl_sombra" aria-hidden="true"></i>
                                    </span>
                                    <span class="info-box-icon mx-auto infospan">
                                      <h3 style="font-family: 'Play';font-weight: 600;font-size: 2.15rem; text-shadow: 1px 1px #000;width: 200px;" id="descrip_tramite">TRAMITES VARIOS</h3>
                                    </span>
                                    {% set TramiteSeleccionado='VARIOS' %}
                                {% endif %}

                                {% if tipoTramite == "G"%}
                                    <span class="info-box-icon pb_1 h-100 mx-auto infoicono">
                                        <i class="ima_posicion fa fa-blind cl_sombra" aria-hidden="true"></i>
                                    </span>
                                    <span class="info-box-icon mx-auto infospan">
                                      <h3 style="font-family: 'Play';font-weight: 600;font-size: 2.15rem; text-shadow: 1px 1px #000;width: 200px;" id="descrip_tramite">GERIATRICO</h3>
                                    </span>
                                    {% set TramiteSeleccionado='GERIATRICO' %}
                                {% endif %}
                            </div>
                        </div>
                    </div>
                </div>

                <div class="row justify-content-center align-items-center mt-2"  id="hijo_opcion" name="hijo_opcion">
                    {%set linea=0%}
                    {%for motivo in datosMotivos%}
                        {%set linea=linea + 1%}
                        <div class="col-12 mt-1" id="hijo_t1" name="hijo_t1" onclick="confirmar('{{ tipoTramite }}',{{ motivo.idMotivo }}, '{{ motivo.descripcion }}');">
                            {% if tipoTramite == "J"%}
                                {%set tcolor="bg-primary"%}
                                {%set bcolor="#007bff !important"%}
                                {% set scolor="#fff" %}
                                    <div class="info-box bg-primary pd-4 pb-4" >
                            {% endif %}

                            {% if tipoTramite == "E"%}
                                {%set tcolor="bg-green"%}
                                {%set bcolor="#00a65a !important"%}
                                {% set scolor="#fff" %}
                                    <div class="info-box bg-green pd-4 pb-4" >
                            {% endif %}

                            {% if tipoTramite == "P"%}
                                {%set tcolor="bg-yellow"%}
                                {%set bcolor="#f39c12 !important"%}
                                {% set scolor="#fff" %}
                                    <div class="info-box bg-yellow pd-4 pb-4" >
                            {% endif %}

                            {% if tipoTramite == "C"%}
                                {%set tcolor="bg-info"%}
                                {%set bcolor="#17a2b8!important"%}
                                {% set scolor="#17a2b8!important" %}
                                    <div class="info-box bg-info pd-4 pb-4" >
                            {% endif %}

                            {% if tipoTramite == "M"%}
                                {%set tcolor="bg-danger"%}
                                {%set bcolor="#dc3545!important"%}
                                {% set scolor="#dc3545!important" %}
                                    <div class="info-box bg-danger pd-4 pb-4" >
                            {% endif %}
                            {% if tipoTramite == "T"%}
                               {%set tcolor="bg-secondary"%}
                               {%set bcolor="#6c757d!important"%}
                               {% set scolor="#fff" %}
                                    <div class="info-box pd-4 pb-4" style="background-color: #6c757d!important; color: #fff;">
                            {% endif %}

                           {% if tipoTramite == "V"%}
                               {%set tcolor="bgvarios"%}
                               {%set bcolor="#e1e6ea!important"%}
                               {% set scolor="#2d2727!important;" %}
                                    <div class="info-box bgvarios pd-4 pb-4" style="background-color: #e1e6ea!important; color: #2d2727!important;">
                           {% endif %}


                            {% if tipoTramite == "G"%}
                                        {%set tcolor="bggeriatrico"%}
                                        {%set bcolor="#9c27b0!important"%}
                                        {% set scolor="#fff!important;" %}
                                        <div class="info-box bggeriatrico pd-4 pb-4" style="background-color: #9c27b0!important; color: #fff!important;">
                            {% endif %}
                                        <!--<span class="info-box-icon h-100">
                                            <i aria-hidden="true" class="sombra_numeros">{ linea }}</i>
                                        </span>-->
                                       <span class="info-box-icon h-100" style="background-color: #fff; "><!--border-top: 1px solid { bcolor }}; border-left: 1px solid { bcolor }}; border-bottom: 1px solid { bcolor }};-->
                                            <span class="info-box-icon h-100 {{ tcolor }}" style="border-radius: 50%; margin-left: 43px;">
                                                <i aria-hidden="true" class="sombra_numeros" style="color:{{ scolor }}">{{ linea }}</i>
                                            </span>
                                       </span>
                                       <div class="info-box-content">
                                            <span class="info-box-text cl_sombra" style="color:{{ scolor }}"><h1 class="h_padre"></h1> {{ motivo.descripcion }}</span>
                                       </div>
                                    </div>
                        </div>
                    {%endfor%}
                    <!--<div class="row">-->
                        <div class="col-12 mt-2" id="div_volver" name="div_volver" >
                            <div class="info-box bg-dark">
                                <span class="info-box-icon">
                                    <i class="fa fa-arrow-left cl_sombra" aria-hidden="true" style="color: #fff!important;"></i>
                                </span>
                                <div class="info-box-content" style="border-top: 1px solid #e1e6ea!important;  border-left: 1px solid #e1e6ea!important;   border-bottom: 1px solid #e1e6ea!important;    border-right: 1px solid #e1e6ea!important;">
                                    <span class="info-box-text cl_sombra" style="margin-top: 0.3em!important; margin-bottom: 0.1em!important;"><label style="color:#6c757d;">VOLVER</label></span>
                                </div>
                            </div>
                        </div>
                    <!--</div>-->
                </div>

                <div id="div_confirmar" class="modal fade modal_tk" role="dialog">
                    <div class="modal-dialog modal-lg" style="width: 600px;">
                        <!-- Modal content-->
                        <div class="modal-content">
                            <div class="modal-header" id="modal_impre" style="color:#fff!important;">
                                <h4 class="modal-header modal-header-header text-center" id="htxt" style="margin: 0 auto;">¿ESTA SEGURO?</h4>
                            </div>
                            <div class="modal-body">
                                <p class="text-center" style="font-weight: bold; font-size: 22px;">USTED SELECCIONO:</p>
                                <p class="text-center" id="lbl_tramite" style="text-shadow: 1px 1px #000;  border-left: none; border-top: none; font-weight: bold; font-size: 34px; font-weight: 600;">- {{ TramiteSeleccionado }} -</p>
                                <p class="text-center" id="lbl_descrip_seleccionada" style="font-family: 'Play'; text-transform: uppercase!important; padding: 10px; font-size: 30px; text-shadow: 1px 1px #000; font-weight: 600; color: #fff!important;"></p>
                                <label class="control-label" id="lbl_tipo_tramite" style="color: #a94442; display:none"></label>
                                <label class="control-label" id="lbl_motivo" style="color: #a94442; display:none"></label>
                            </div>
                            <div class="modal-footer justify-content-between">
                                <button type="button" class="btn btn-success btn_imprimir" data-dismiss="modal" id="btaceptar" onclick="javascript:AltaTicket()" ><i class="fa fa-check" aria-hidden="true"></i> IMPRIMIR</button>
                                <button type="button" class="btn btn-danger btn_cancelar" data-dismiss="modal" ><i class="fa fa-times" aria-hidden="true"></i> CANCELAR</button>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="modal fade" tabindex="-1" role="dialog" id="div_modal_ticket">
                    <div class="modal-dialog modal-dialog-centered" role="document">
                        <div class="modal-content" >
                            <div class="modal-header" style="background-color: #f39c12 !important; color: #fff;">
                                <h3 class="modal-title" id="lbl_imprimiendo" style="margin: 0 auto;">IMPRIMIENDO TICKET...</h3>
                            </div>
                            <div class="modal-body" >
                                <div class="progress">
                                    <div class="progress-bar progress-bar-striped progress-bar-animated" role="progressbar" style="width: 100%" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="modal" tabindex="-1" role="dialog" id="divModalConfTicket">
                    <div class="modal-dialog modal-dialog-centered" role="document">
                        <div class="modal-content" >
                            <div class="modal-header" style="background-color: #f39c12 !important; color: #fff;">
                               <h3 class="modal-title" style="margin: 0 auto;">TICKET GENERADO</h3>
                            </div>
                            <div class="modal-body" style="text-align: center;">
                                <h3>NRO. TICKET:</h3> <h3 id="h_nrotk" style="font-size: 40px;"></h3>
                               <h5 class="modal-title mt-4">POR FAVOR RETIRE SU TICKET.- GRACIAS</h5>
                            </div>
                        </div>
                    </div>
                </div>


                <input type="text" id="txt_tipo" value="0" style="display:none;">
                <input type="text" id="txt_sn" value="0" style="display:none;">
            </div>

            <div id="Imprime" class="imprime" style="display: none;">

                <p style="text-align: center; margin:0px; padding-top: 0px !important;">DIRECCIÓN DE RENTAS</p>
                <br>
                -----------------------------------------------
                <br>
                <p style="text-align: center; margin:0px; font-size: 12px;" id="p_bienvenido"></p>
                <p style="text-align: center; margin:0px; font-size: 12px;" id="apeynom_sel"></p> <br>
                <p style="text-align: center; margin:0px;">NRO.TICKET</p>
                <h2 style="font-size:70px; font-weight:bolder; margin:0px; text-align: center;" id="nro_tk"></h2>
                <p style="text-align: center; margin:0px; font-size: 12px;" id="tramite_sel"></p>

                <br>
                <p style="text-align: center; margin:0px; padding-top: 0px !important; font-size: 12px;">Emision: {{ "Now"|date('d/m/Y H:i') }}</p>
                <p style="text-align: center; margin:0px; padding-top: 0px !important; font-size: 12px;">Por favor, conseve su ticket</p>
                <br>
                <p style="text-align: center; margin:0px; padding-top: 0px !important; font-size: 12px; font-weight: bold">0800-999-6864</p>
                <p style="text-align: center; margin:0px; padding-top: 0px !important; font-size: 12px;">Lu a Vi de 8:00 a 17:00 hs</p>
                <p style="text-align: center; margin:0px; padding-top: 0px !important; font-size: 12px;">www.chivilcoy.gov.ar/rentas</p>
                <p style="text-align: center; margin:0px; padding-top: 0px !important; font-size: 12px;">rentas@chivilcoy.gov.ar</p>
                -----------------------------------------------
                <p style="text-align: center; margin:0px; font-size: 12px;">Municipalidad de Chivilcoy</p> <br>
            </div>
            <!-- Footer -->
            <footer class="main-footer d-none d-md-block d-lg-block mt-4" style="position: absolute; bottom: 0px;">
                Desarrollado por la Dirección de Informática de la Municipalidad de Chivilcoy
            </footer>

    {% endblock %}

